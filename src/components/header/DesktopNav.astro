---
type NavItem = { navLabel: string; href: string };

interface DesktopNavProps {
  links: NavItem[];
}

const { links } = Astro.props as DesktopNavProps;
---

<ul class="flex gap-x-12">
  {
    links.map(({ navLabel, href }) => (
      <li>
        <a
          href={href}
          class="inline-block text-sm font-semibold transition duration-200  hover:text-accentHover hover:scale-105"
        >
          {navLabel}
        </a>
      </li>
      
    ))
  }
</ul>
